<!DOCTYPE html>
<html lang="en">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <!-- Meta, title, CSS, favicons, etc. -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Board Members</title>

<%- include('../common/stylesheets'); %>
  <style type="text/css">
    [v-cloak] {
      display: none;
    }
    .member-position-label {
      padding: 7px;
    }
  </style>
</head>

<body class="nav-md">
  <div class="container body">
    <div class="main_container">
        <%- include('../common/resident-navigation'); %>

      <!-- page content -->
      <div class="right_col" role="main">
        <div class="">
          <div class="page-title">
            <div class="title_left" style="width:100%">
              <h3><i class="fa fa-group members-icon"></i>Board of Members</h3>
            </div>
            <!--
                            <div class="title_right">
                                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search for...">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button">Go!</button>
                                        </span>
                                    </div>
                                </div>
                            </div> <!-- title right -->
          </div>
          <!-- page title -->

          <div class="clearfix"></div>

          <div class="">

            <div class="clearfix"></div>

            <div class="row">
              <div class="col-md-12">
                <div class="x_panel" style="padding-top:22px;">
                  <div class="x_content">
                  <% if (!data.error) {  data.forEach(function (member){
                    if (member.position == "President") { %>


                    <div class="row">
                      <div class="col-md-4 col-md-offset-4 col-sm-4 col-sm-offset-4 col-xs-12 profile_details">
                        <div class="well profile_view members-detail">
                          <div class="col-sm-12">
                            <h4 class="brief"><%= member.name %></h4>
                            <div class="right col-xs-5 member-img-padding">
                              <img src="images/img.jpg" alt="" class="member-img img-responsive">
                            </div>
                            <div class="left col-xs-7">
                            <span class="label label-info member-position-label"><%= member.position %></span>

                              <hr style="margin-top:8px; margin-bottom:8px;">

                              <ul class="list-unstyled contact_member">
                                <li><i class="fa fa-mobile-phone icon_mobile"></i><%= member.contactPhone %></li>
                                <li><i class="fa fa-envelope"></i><%= member.contactEmail %></li>
                              </ul>
                            </div>
                          </div>
                        </div>
                        <!-- well profile_view -->
                      </div>
                      <!-- profile_details -->
                    </div>

                    <% } }); }%>

                     <div class="row">
                       <% if (!data.error) {  data.forEach(function (member){
                    if (member.position != "Member" && member.position !="President") { %>
                      <div class="col-md-3  col-sm-4  col-xs-12 profile_details">
                        <div class="well profile_view members-detail">
                          <div class="col-sm-12">
                            <h4 class="brief"><%= member.name %></h4>
                            <div class="right col-xs-5 member-img-padding">
                              <img src="images/img.jpg" alt="" class="member-img img-responsive">
                            </div>
                            <div class="left col-xs-7">
                            <span class="label label-warning member-position-label"><%= member.position %></span>

                              <hr style="margin-top:8px; margin-bottom:8px;">

                              <ul class="list-unstyled contact_member">
                                <li><i class="fa fa-mobile-phone icon_mobile"></i><%= member.contactPhone %></li>
                                <li><i class="fa fa-envelope"></i><%= member.contactEmail %></li>
                              </ul>
                            </div>
                          </div>
                        </div>
                        <!-- well profile_view -->
                      </div>
                      <!-- profile_details -->

                    <% } }); }%>
                    </div>



                      <!-- 3rd level -->
 <div class="row">
                       <% if (!data.error) {  data.forEach(function (member){
                    if (member.position == "Member") { %>
                      <div class="col-md-3  col-sm-4  col-xs-12 profile_details">
                        <div class="well profile_view members-detail">
                          <div class="col-sm-12">
                            <h4 class="brief"><%= member.name %></h4>
                            <div class="right col-xs-5 member-img-padding">
                              <img src="images/img.jpg" alt="" class="member-img img-responsive">
                            </div>
                            <div class="left col-xs-7">
                            <span class="label label-danger member-position-label"><%= member.position %></span>

                              <hr style="margin-top:8px; margin-bottom:8px;">

                              <ul class="list-unstyled contact_member">
                                <li><i class="fa fa-mobile-phone icon_mobile"></i><%= member.contactPhone %></li>
                                <li><i class="fa fa-envelope"></i><%= member.contactEmail %></li>
                              </ul>
                            </div>
                          </div>
                        </div>
                        <!-- well profile_view -->
                      </div>
                      <!-- profile_details -->

                    <% } }); }%>
                    </div>

                  </div>
                </div>
              </div>
            </div>

          </div>



          <!-- /page content -->
        </div>
      </div>
      <!-- right_col -->

      <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
      </div>
    </div>
    <!-- main container -->
  </div>
  <!-- container body -->

  <!-- jQuery -->
  <script src="/vendors/jquery/dist/jquery.min.js"></script>
  <!-- Bootstrap -->
  <script src="/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
  <!-- FastClick -->
  <script src="/vendors/fastclick/lib/fastclick.js"></script>
  <!-- NProgress -->
  <script src="/vendors/nprogress/nprogress.js"></script>
  <!-- bootstrap-progressbar -->
  <script src="/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
  <!-- iCheck -->
  <script src="/vendors/iCheck/icheck.min.js"></script>
  <!-- PNotify -->
  <script src="/vendors/pnotify/dist/pnotify.js"></script>
  <script src="/vendors/pnotify/dist/pnotify.buttons.js"></script>
  <script src="/vendors/pnotify/dist/pnotify.nonblock.js"></script>
  <script type="text/javascript" src="/vendors/serializejson.min.js"></script>
  <!-- Custom Theme Scripts -->
  <script src="/js/custom.min.js"></script>


</body>

</html>
